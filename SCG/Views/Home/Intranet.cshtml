        padding: 20px 20px 60px 20px;
      }

      #footer p {
        text-align: center;
        color: white;
      }


    #cards {
      list-style: none;
      margin: 0;
      padding: 0;
    }

      #cards li {
        position: relative;
        float: left;
        width: 31%;
        min-height: 340px;
        margin: 1%;
        padding: 0;
        font-size: 14px;
        background-color: white;
        border: #CCC solid 1px;
        border-radius: 8px;
      }

        #cards li .handle {
          display: block;
          padding: 10px 20px;
          font-size: 14px;
          background-color: #ebdfc2;
          border-radius: 4px;
          cursor: move;
        }

        #cards li .check {
          position: absolute;
          top: 6px;
          right: 7px;
          display: block;
          padding: 5px 10px;
          font-size: 12px;
          background-color: #dfcca3;
          border: white solid 1px;
          border-radius: 4px;
          cursor: pointer;
        }

          #cards li .check:hover {
            border-color: darkgoldenrod;
          }

        #cards li .content {
          display: block;
          padding: 20px;
          font-size: 14px;
          max-height: 261px;
          overflow-y: auto;
        }

        #cards li .content img {
          height: auto;
          max-width: 100%;
        }

        #cards li .content hr {
          margin: 15px;
        }
  </style>

