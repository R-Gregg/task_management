  <style>
    * {
      margin: 0;
      padding: 0;
      font-family: "open_sansregular", Arial, Helvetica, sans-serif;
      font-size: 0;
      color: #333;
    }

    h1, h2, h3, h4 {
      margin: 0 0 20px 0;
      font-family: "quicksandbook",Arial, Helvetica, sans-serif;
      text-transform: capitalize;
    }

    h1 {
      font-size: 24px;
      margin-bottom: 20px;
    }

    h2 {
      font-size: 18px;
      margin-bottom: 20px;
    }

    h3 {
      font-size: 16px;
      margin-bottom: 20px;
    }

    h4 {
      font-size: 14px;
      margin-bottom: 20px;
    }

    p, ul, ol {
      font-size: 14px;
      margin-bottom: 20px;
    }

    #header {
      position: fixed;
      z-index: 10;
      top: 0;
      left: 0;
      width: 100%;
    }

    #info-bar {
      position: relative;
      z-index: 10;
    }

      #info-bar > div {
        padding: 5px 20px;
        font-size: 12px;
        color: white;
        background-color: #1f2122;
      }

      #info-bar a, #info-bar i {
        padding: 0 1px;
        font-size: 12px;
        color: #888;
        text-decoration: none;
      }

        #info-bar a:hover, #info-bar i:hover {
          color: #FFF;
        }

    #hero {
    }

      #hero #hero-image {
        width: 100vw;
        height: 60vh;
        background: url(/images/Saltmarsh-7874.JPG) no-repeat center right;
        background: url(/images/Saltmarsh-7516.JPG) no-repeat center right;
        background-size: cover;
        border-bottom: #000 solid 1px;
      }

        #hero #hero-image #logo {
          position: absolute;
          top: 50px;
          left: 20px;
          height: 27%;
          max-height: 150px;
          padding-top: 0px;
        }

        #hero #hero-image #hero-down {
          position: absolute;
          top: 50px;
          right: 20px;
          font-size: 24px;
          padding: 5px 10px;
          color: white;
          background-color: #000;
          border-radius: 50px;
          opacity: .65;
        }


    #hero-bumper {
      height: 60vh;
      margin-bottom: 100px;
    }

    #nav {
    }

      #nav #nav-1 {
        background-color: #1f2122;
      }

        #nav #nav-1 ul {
          list-style: none;
          text-align: center;
          margin: 0;
        }

          #nav #nav-1 ul li {
            display: inline-block;
          }

            #nav #nav-1 ul li a {
              display: block;
              padding: 10px 2vw;
              font-size: 14px;
              color: white;
              text-decoration: none;
              text-shadow: 1px 1px 2px #000;
            }

              #nav #nav-1 ul li a:hover {
                background-color: #4b4e4e;
              }

      #nav #nav-2 {
        background-color: #FFF;
        border-top: darkgoldenrod solid 2px;
      }

        #nav #nav-2 ul {
          list-style: none;
          text-align: center;
          margin: 0;
        }

          #nav #nav-2 ul li {
            display: inline-block;
          }

            #nav #nav-2 ul li a {
              display: block;
              padding: 10px 2vw;
              font-size: 14px;
              color: darkgoldenrod;
              text-decoration: none;
              font-weight: bold;
            }

              #nav #nav-2 ul li a:hover {
                color: #1f2122;
                background: url(/images/caret.png) center top -24px no-repeat;
              }

    #body {
    }

      #body > div {
        display: inline-block;
        vertical-align: top;
      }

        #body > div > div {
          margin: 0 10px;
          padding: 20px;
          background-color: #ebdfc2;
        }

      #body #body-1 {
        width: 25%;
      }

        #body #body-1 > div {
        }

      #body #body-2 {
        width: 50%;
      }

        #body #body-2 > div {
